<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--  SignUpActivity Style  -->
    <style name="introduce_desc">
        <item name="android:fontFamily">@font/inter_semibold</item>
        <item name="android:textColor">@color/text_color_navy</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="start_button">
        <item name="android:fontFamily">@font/inter_semibold</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="login_question">
        <item name="android:fontFamily">@font/inter</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">12sp</item>
    </style>
    <style name="login_desc">
        <item name="android:fontFamily">@font/inter_semibold</item>
        <item name="android:textColor">@color/main_color</item>
        <item name="android:textSize">12sp</item>
    </style>
    <!--  SocialLoginButton Style  -->
    <style name="social_login_kakao_button">
        <item name="android:fontFamily">@font/inter_semibold</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="social_login_facebook_button">
        <item name="android:fontFamily">@font/inter_semibold</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
    </style>
